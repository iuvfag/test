import java.util.HashMap;
import java.util.Map;


public class MapSample {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Map<String,String> map = new HashMap<String,String>();


		//値のセットはputメソッドを利用
		map.put("key1","value1");
		map.put("key2", "value2");
		map.put("key3", "value3");
		map.put("key4", "value4");
		map.put("key5", "value5");

	}

}
