import java.util.ArrayList;

public class List {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		List<String> list = new ArrayList<String>();

		//値の記述はaddメソッドを利用
		list.add("1");
		list.add("2");
		list.add("3");
		list.add("4");
		list.add("5");

		//値の取得
		for(int i=0; i<list.size() -1; i++){
			//値の取得はgetメソッドを利用
			System.out.println(list.get(i));
		}

		for(String s : list){
			System.out.println(s);
		}

	}

}
